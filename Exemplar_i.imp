IMPLEMENTATION Exemplar_i
REFINES Exemplar
VALUES TITULO = 1..10
       
IMPORTS SetsExemplar(TITULO), SetsTitulo(TITULO)
INVARIANT sete = exemplar & 
          set2 = titulo
    
OPERATIONS
   
   adicionarTitulo(ll) =
    BEGIN
       add2(ll) 
        END;
   
   
    AdicionarExemplar(ll, nn) =
    BEGIN
       add(ll,nn) 
        END
        
     

 END